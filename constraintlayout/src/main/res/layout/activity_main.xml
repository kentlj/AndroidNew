<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.learn.constraintlayout.MainActivity"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <Button
        android:id="@+id/button6"
        android:layout_width="88dp"
        android:layout_height="48dp"
        android:text="Button"
        android:layout_marginLeft="109dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="83dp"
        android:layout_marginStart="109dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.16"/>

    <Button
        android:id="@+id/button7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        app:layout_constraintRight_toRightOf="@+id/button6"
        app:layout_constraintHorizontal_bias="1.0"
        android:layout_marginTop="84dp"
        app:layout_constraintTop_toBottomOf="@+id/button6"/>

    <Button
        android:id="@+id/button8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        android:layout_marginTop="78dp"
        app:layout_constraintTop_toBottomOf="@+id/button7"
        app:layout_constraintRight_toRightOf="@+id/button7"/>

    <TextView
        android:id="@+id/textView"
        android:layout_width="84dp"
        android:layout_height="38dp"
        android:text="TextView"
        app:layout_constraintLeft_toRightOf="@+id/button6"
        android:layout_marginLeft="63dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="103dp"/>

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline2"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.22916667"/>

</android.support.constraint.ConstraintLayout>
